  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "typeorm": "ts-node --transpile-only -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/configuration/dbconfig.ts",
    "schema:sync": "npm run typeorm schema:sync",
    "schema:drop": "npm run typeorm schema:drop",
    "add:migration": "npm run typeorm migration:generate",
    "apply:migration": "npm run typeorm migration:run",
    "revert:migration": "npm run typeorm migration:revert",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bluebird": "^3.5.42",
    "@vitalets/google-translate-api": "^9.2.0",
    "async-retry": "^1.3.3",
    "axios": "^1.4.0",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "deep-object-diff": "^1.1.9",
    "discord.js": "^14.11.0",
    "dotenv": "^16.2.0",
    "dotenv-safe": "^8.2.0",
    "ioredis": "^5.3.2",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "lodash-contrib": "^4.1200.1",
    "md5": "^2.3.0",
